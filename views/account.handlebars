<main id="account">
    {{>header}}

    <section>
        {{ h1 }}
        <table>
            <thead>
                <tr>
                    <th>Username</th>
                    <th>Email</th>
                    <th>Passwod</th>
                </tr>
            </thead>

            <tbody>
                {{#each list}}
                <tr>
                    <td>{{this.username}}</td>
                    <td>{{this.email}}</td>
                    <td>{{this.password}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </section>
</main>